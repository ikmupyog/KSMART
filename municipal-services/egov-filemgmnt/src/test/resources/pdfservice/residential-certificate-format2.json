{
  "key": "residential-certificate",
  "config": {
    "pageSize": "A4",
    "pageOrientation": "portrait",
    "pageMargins": [70.86, 70.86, 70.86, 70.86],
    "defaultStyle": {
      "font": "Thumba"
    },
    "content": [
      {
        "table": {
          "widths": [
            "*"
          ],
          "body": [
            [
              {
                "stack": [
                  {
                    "image": "data:image/jpeg;base64,",
                    "width": 70,
                    "height": 70,
                    "style": "logo-img"
                  }
                ]
              }
            ]
          ]
        },
        "layout": "noBorders"
      },
      {
        "table": {
          "widths": [
            "*"
          ],
          "body": [
            [
              {
                "stack": [
                  {
                    "text": "{{tenantName}}",
                    "style": "pdf-header"
                  },
                  {
                    "text": "{{lbAddressWithPinCode}}",
                    "style": "pdf-sub-header"
                  }
                ]
              }
            ]
          ]
        },
        "layout": "noBorders"
      },
      {
        "table": {
          "widths": [
            "*"
          ],
          "body": [
            [
              {
                "text": "റസിഡൻഷ്യൽ സർട്ടിഫിക്കറ്റ്",
                "style": "title-header",
                "decoration": "underline"
              }
            ]
          ]
        },
        "layout": "noBorders"
      },
      {
        "style": "title-sub-header-margin",
        "table": {
          "widths": [
            "*", "auto"
          ],
          "body": [
            [
              {
                "stack": [
                  {
                    "text": "സർട്ടിഫിക്കറ്റ് നമ്പർ: {{certificateNumber}}",
                    "style": ["title-sub-header", "align-left"]
                  }
                ]
              },
              {
                "stack": [
                  {
                    "text": "Date: {{certificateDate}}",
                    "style": ["title-sub-header", "align-right"]
                  }
                ]
              }
            ]
          ]
        },
        "layout": "noBorders"
      },
      {
        "table": {
          "widths": [
            "*"
          ],
          "body": [
            [
              {
                "columns": [
                  {
                    "text": "ശ്രീ /ശ്രീമതി {{name}}  ,{{address}}  (കെട്ടിട ഉടമസ്ഥന്റെ പേര്,വിലാസം )  യുടെ ഉടമസ്ഥതയിലുള്ള  ബിൽഡിംഗ് ഐഡി യിൽ  വാർഡിൽ നമ്പർ കെട്ടിടത്തിൽ ശ്രീ /ശ്രീമതി (അപേക്ഷകന്റെ പേര്, മേൽവിലാസം ) (മാസം /വര്ഷം ) കാലയളവിൽ താമസിച്ചു വരുന്നു .",
                    "style": "certificate-body"
                  }
                ]
              }
            ]
          ]
        },
        "layout": "noBorders"
      },
      {
        "style": "title-sub-header-margin",
        "table": {
          "widths": [
            "*"
          ],
          "body": [
            [
              {
                "stack": [
                  {
                    "text": "Signature",
                    "style": ["title-sub-header", "align-right"]
                  }
                ]
              }
            ]
          ]
        },
        "layout": "noBorders"
      }
    ],
    "styles": {
      "logo-img": {
        "alignment": "center",
        "margin": [0, 0, 0, 0]
      },
      "pdf-header": {
        "color": "#000000",
        "bold": true,
        "fontSize": 14,
        "fontWeight": 400,
        "alignment": "center",
        "margin": [0, 20, 0, 0]
      },
      "pdf-sub-header": {
        "color": "#000000",
        "bold": true,
        "fontSize": 14,
        "fontWeight": 400,
        "alignment": "center",
        "margin": [0, 8, 0, 0]
      },
      "title-header": {
        "color": "#000000",
        "bold": true,
        "fontSize": 14,
        "alignment": "center",
        "margin": [0, 25, 0, 0]
      },
      "title-sub-header": {
        "color": "#000000",
        "bold": true,
        "fontSize": 11,
        "fontWeight": 400,
        "margin": [0, 0, 0, 0]
      },
      "title-sub-header-margin": {
        "margin": [0, 45, 0, 45]
      },
      "certificate-body": {
        "color": "#000000",
        "bold": false,
        "fontSize": 11,
        "letterSpacing": 2.74,
        "fontWeight": 400,
        "margin": [0, 0, 0, 0]
      },
      "align-left": {
         "alignment": "left"
      },
      "align-right": {
         "alignment": "right"
      }
    }
	}
}