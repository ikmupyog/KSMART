{
  "key": "residential-certificate",
  "DataConfigs": {
    "serviceName": "rainmaker-common",
    "version": "1.0.0",
    "baseKeyPath": "$.ResidentialCertificate.*",
    "entityIdPath": "$.id",
    "isCommonTableBorderRequired": true,
    "mappings": [
      {
        "topic": "common-pdf-generation-3",
        "mappings": [
          {
            "direct": [
              {
                "variable": "tenantLogo",
                "url": "https://ulb-logos.s3.ap-south-1.amazonaws.com/kerala_logo.jpg",
                "type": "image"
              },
              {
                "variable": "waterMarkImage",
                "url": "https://ulb-logos.s3.ap-south-1.amazonaws.com/kerala_logo.jpg",
                "type": "image"
              },
              {
                "variable": "tenantName",
                "value": {
                  "path": "$.lbName"
                },
                "type": "setEmpty"
              },
              {
                "variable": "tenantAddress",
                "value": {
                  "path": "$.lbAddressWithPinCode"
                },
                "type": "setEmpty"
              },
              {
                "variable": "applicantName",
                "value": {
                  "path": "$.name"
                }
              },
              {
                "variable": "applicantAddress",
                "value": {
                  "path": "$.address"
                },
                "type": "setEmpty"
              },
              {
                "variable": "embeddedUrl",
                "value": {
                  "path": "$.embeddedUrl"
                },
                "type": "setEmpty"
              },
              {
                "variable": "tenantId",
                "value": {
                  "path": "$.tenantId"
                },
                "localisation": {
                  "required": true,
                  "prefix": "TENANT_TENANTS",
                  "module": "rainmaker-common"
                }
              },
              {
                "variable": "certificateDate",
                "value": {
                  "path": "$.dateOfIssue"
                },
                "type": "date",
                "format": "DD/MM/YYYY"
              }
            ]
          },
          {
            "qrcodeConfig": [
              {
                "variable": "qrCodeImage",
                "value": "{{embeddedUrl}}"
              }
            ]
          }
        ]
      }
    ]
  }
}